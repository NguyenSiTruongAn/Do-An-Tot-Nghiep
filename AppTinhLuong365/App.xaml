<Application x:Class="AppTinhLuong365.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AppTinhLuong365"
             xmlns:math="clr-namespace:AppTinhLuong365.Core"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <math:MathConvert x:Key="MathConvert"/>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="/Resouces/Color/Default.xaml"/>
                <ResourceDictionary Source="/Resouces/Icon.xaml"/>
                <ResourceDictionary Source="/Resouces/language/ResourceString.vi-VN.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
